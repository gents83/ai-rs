    "version": "0.2.0",
    "configurations": [
        {
            "name": "RUN DEBUG 'burn-empty'",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "cargo",
            "args": [
                "run",
                "--example",
                "empty",
            ],
            "cwd": "${workspaceFolder}/crates/",
            "requireExactSource": false,
            "preLaunchTask": "COMPILE DEBUG",
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/crates/target//debug/",
        },
        {
            "name": "RUN DEBUG 'candle-gemma'",
